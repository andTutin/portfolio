.header {
    grid-column: 1 / 13;
    grid-row: 1 / 2;
    display: flex;
    justify-content: space-between;
    align-items: center;

  &__menu {
    width: 690px;
    flex-shrink: 0;
  }

  &__socials {
    width: 165px;
  }

  &__socials .socials__item {
    border: transparent;

    &:hover {
      border: transparent;
    }

  }
}

